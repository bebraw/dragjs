            {
              "element": "div",
              "class": "flex flex-col gap-8",
              "children": [
                {
                  "element": "h1",
                  "class": "text-4xl md:text-8xl",
                  "children": [
                    {
                      "element": "span",
                      "class": "whitespace-nowrap pr-4",
                      "children": "âœ¥"
                    },
                    {
                      "element": "span",
                      "children": "dragjs"
                    }
                  ]
                },
                {
                  "element": "h2",
                  "class": "text-xl md:text-4xl font-extralight",
                  "children": "Easy dragging with JavaScript"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl",
